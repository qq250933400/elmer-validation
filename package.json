{
  "name": "elmer-validation",
  "version": "1.0.3",
  "description": "form validation",
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "test": "mocha  --reporter mochawesome",
    "cover": "nyc --reporter=html mocha",
    "lib": "tsc -w",
    "build": "tsc",
    "static": "node ./bin/index.js --static"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "keywords": [
    "validation",
    "elmerUI",
    "react",
    "angular"
  ],
  "author": "elmer mo",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.6.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nyc": "^14.1.1",
    "tslint": "^5.11.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@abraham/reflection": "^0.4.2",
    "@types/chai": "^4.2.3",
    "@types/core-js": "^2.5.0",
    "@types/mocha": "^5.2.7",
    "ajv": "5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "core-js": "^2.5.7",
    "decorator": "^0.0.1",
    "elmer-common": "^0.0.4",
    "eslint-plugin-typescript": "^0.12.0",
    "jsdom": "^15.2.0",
    "mocha": "^6.2.1",
    "mochawesome": "^4.1.0",
    "reflect-metadata": "^0.1.12",
    "ts-node": "^8.4.1",
    "typescript-eslint-parser": "^18.0.0",
    "zone.js": "^0.8.26"
  }
}
